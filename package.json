{
  "name": "layout-service",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rm -Rf ./build && tsc --build",
    "start:dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddie-kag/layout-service.git"
  },
  "author": "Eddie Kagrimanyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eddie-kag/layout-service/issues"
  },
  "homepage": "https://github.com/eddie-kag/layout-service#readme",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@api/*": "build/api",
    "@config/*": "build/config",
    "@controllers/*": "build/controllers",
    "@db/*": "build/db",
    "@models/*": "build/models",
    "@routes/*": "build/routes",
    "@shared/*": "build/shared"
  },
  "dependencies": {
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.2",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
